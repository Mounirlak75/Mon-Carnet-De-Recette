{{ form_start(form, {'attr': {'class': 'my-form'}}) }}

{% for child in form.children %}
    <div class="form-group">
        {{ form_label(child, null, {'label_attr': {'class': 'form-label'}}) }}
        {{ form_widget(child, {'attr': {'class': 'form-control'}}) }}
        {{ form_errors(child) }}
    </div>
{% endfor %}

<button class="btn btn-primary">{{ button_label|default('Valider') }}</button>

{{ form_end(form) }}
